                    <to-view-id>/jsp/workbench/MHLResponseView.jsp</to-view-id>
          </navigation-case>
          <navigation-case>
		             <from-action>#{ncicResponseView.doInitiateNCICCheck}</from-action>
		             <from-outcome></from-outcome>
		             <to-view-id>/jsp/workbench/NCICResponseView.jsp</to-view-id>
          </navigation-case> 
          <navigation-case>
		             <from-action>#{ncicResponseView.doReset}</from-action>
