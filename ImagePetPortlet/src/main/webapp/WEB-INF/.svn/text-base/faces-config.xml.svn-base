          </navigation-case>          
          <navigation-case>
                    <from-action>#{ncicResponseView.doDisplayNICSCheck}</from-action>
                    <from-outcome>displayNICSCheck</from-outcome>
                    <to-view-id>/jsp/workbench/NICSResponseView.jsp</to-view-id>
          </navigation-case>
           <navigation-case>
                    <from-action>#{ncicResponseView.doDisplayDMVCheck}</from-action>
