	<!--  WORKBENCH NAVIGATION -->
	<navigation-rule>
         <from-view-id>/jsp/workbench/DashboardView.jsp</from-view-id>
          <navigation-case>
         			 <from-action>#{pc_DashboardView.adoptAction}</from-action>
                    <from-outcome>RECERTDETAILS</from-outcome>
                    <to-view-id>/jsp/workbench/WorkbenchRecertDetailsView.jsp</to-view-id>
          </navigation-case>          
          <navigation-case>
         			 <from-action>#{pc_DashboardView.recertDetailsAction}</from-action>
                    <from-outcome>RECERTDETAILS</from-outcome>
                    <to-view-id>/jsp/workbench/WorkbenchRecertDetailsView.jsp</to-view-id>
          </navigation-case> 
          <navigation-case>
                    <from-outcome>DASHBOARD</from-outcome>
                    <to-view-id>/jsp/workbench/DashboardView.jsp</to-view-id>
          </navigation-case> 
           <navigation-case>
                    <from-action>#{pc_DashboardView.generateRecertCoversheet}</from-action>
                    <from-outcome>COVERSHEET</from-outcome>
                    <to-view-id>/jsp/workbench/recertCoversheetPDFView.jsp</to-view-id>
          </navigation-case>   
           <navigation-case>
                    <from-action>#{pc_DashboardView.assignRecertApplications}</from-action>
                    <from-outcome>BULKASSIGN</from-outcome>
                    <to-view-id>/jsp/workbench/recertSumamryPageForGenerate.jsp</to-view-id>
          </navigation-case>                          
    </navigation-rule>
    <navigation-rule>
         <from-view-id>/jsp/workbench/recertSumamryPageForGenerate.jsp</from-view-id>
          <navigation-case>
                    <from-action>#{recertSumamryView.generateRecertCoversheet}</from-action>
                    <from-outcome>COVERSHEET</from-outcome>
                    <to-view-id>/jsp/workbench/recertCoversheetPDFView.jsp</to-view-id>
          </navigation-case>  
          <navigation-case>
                    <from-action>#{recertSumamryView.goToDashboard}</from-action>
                    <from-outcome>DASHBOARD</from-outcome>
                    <to-view-id>/jsp/workbench/DashboardView.jsp</to-view-id>
          </navigation-case>  
    </navigation-rule>
    <navigation-rule>
         <from-view-id>/jsp/workbench/WorkbenchRecertDetailsView.jsp</from-view-id>
          <navigation-case>
                    <from-action>#{pc_WorkbenchRecertDetailsView.doFinalReview}</from-action>
                    <from-outcome>FINAL_REVIEW</from-outcome>
                    <to-view-id>/jsp/workbench/FinalReview.jsp</to-view-id>
          </navigation-case>
          <navigation-case>
          			<from-outcome>RECERTDETAILS</from-outcome>
          			<to-view-id>/jsp/workbench/WorkbenchRecertDetailsView.jsp</to-view-id>
          </navigation-case> 
    </navigation-rule>
    	           <!-- Displaying Safe check navigations -->
