    </navigation-rule>
     <!-- Final Review Screen navigations --> 
     <navigation-rule>
          <from-view-id>/jsp/workbench/FinalReview.jsp</from-view-id>          
          <navigation-case>
          			<from-outcome>FINAL_REVIEW</from-outcome>
          			<to-view-id>/jsp/workbench/FinalReview.jsp</to-view-id>
          </navigation-case>
     </navigation-rule>
     <navigation-rule>
      <navigation-case>
                    <from-action>#{pc_WorkbenchRecertDetailsView.goToNeedsPermitHolderReview}</from-action>
