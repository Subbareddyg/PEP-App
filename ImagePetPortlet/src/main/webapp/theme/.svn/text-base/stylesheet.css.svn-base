.documentViewLeftSection{
	Width:30%;
	height:100%;
	float: left;	
	padding-top:10px;
}
.documentViewRightSection{
	Width:69%;
	float: left;
	padding-top:10px;
}
.documentViewRightSection #prevButton{
	float: left;	
	padding-top: 37%;
}
.documentViewRightSection #slideshow{
	width:720px;
	height:800px;
	float: left;	
	position:relative;
	overflow:hidden;
	display:block;
}

.documentViewRightSection #nextButton{
	float: left;
	padding-top: 37%;
}

#slideIframe{
	width:719px;
	height:800px;
	overflow:auto;
}

.documentViewRightSection .next{	
	opacity:0.2;
	filter:alpha(opacity=40);
	width:30px;
}
.documentViewRightSection .next:hover{	
	opacity:1.0;
	filter:alpha(opacity=100);
}
.documentViewRightSection .prev{
	opacity:0.2;
	filter:alpha(opacity=40);
	width:30px;
}

.documentViewRightSection .prev:hover{	
	opacity:1.0;
	filter:alpha(opacity=100);
}
