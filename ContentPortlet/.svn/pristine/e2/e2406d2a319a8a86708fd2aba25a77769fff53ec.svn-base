		$(document).ready(function(){
			
		$( "#iphCategoryDropDownId" ).change(function() {
			
			var url = document.getElementById("getIPHCategory").value;		
			var petIdForWebservice = document.getElementById("petIdForWebservice").value;
			var iphMappingHttpError="The IPH mapping cannot  be updated HTTP Error, contact  the System Administrator";
			var iphMappingSuccess="The IPH mapping has been  updated successfully.";
			var iphMappingUpdateFailed="The IPH mapping cannot be updated,contact the system administrator.";
			
			var ajaxCall=  $.ajax({
				  
			        url: url,
			        type: 'GET',
			        cache:false,			        
			        data: { 
			        	   categoryKey: $("#iphCategoryDropDownId").val(),
			        	   petIdForWebservice:$("#petIdForWebservice").val()
						   },
			            success: function(data){ 
			            	
			            	if (data.indexOf(iphMappingHttpError) > -1 )
				            	{
				            		   
					            	$("#webserviceMessageDescription").html(iphMappingHttpError);
				            	}
			               else if(data.indexOf(iphMappingSuccess) > -1 )
			            		{
			            		   $("#webserviceMessageDescription").html(iphMappingSuccess);
			            		}
			               else if(data.indexOf(iphMappingUpdateFailed) > -1 )
			                   {
			            	   
			            	     $("#webserviceMessageDescription").html(iphMappingUpdateFailed);
			                   }     
                           
			           
			        },
	                error: function(XMLHttpRequest, textStatus, errorThrown){
	                	
	                	$("#webserviceMessageDescription").html(textStatus);
	                  
	                  }
			   
			    });
			 
		  }); 
		});
    </script>
     
	
